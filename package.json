    "url": "https://github.com/fsouza/prettierd/issues"
  },
  "homepage": "https://github.com/fsouza/prettierd",
  "devDependencies": {
    "@types/node": "^17.0.35",
    "@types/prettier": "^2.6.1",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "core_d": "^4.0.0",
    "nanolru": "^1.0.0",
    "prettier": "^2.6.2"
  },
  "files": [
    "bin",
