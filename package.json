{
  "name": "@fsouza/prettierd",
  "version": "0.20.0",
  "description": "prettier, as a daemon",
  "bin": {
    "prettierd": "./bin/prettierd"
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "tsc -b",
    "run": "bin/prettierd",
    "start": "bin/prettierd start",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write ."
