{
  "name": "@fsouza/prettierd",
  "version": "0.26.1",
  "description": "prettier, as a daemon",
  "bin": {
    "prettierd": "./bin/prettierd"
  },
  "scripts": {
    "prepare": "yarn build",
    "build": "tsc -b",
    "run": "node bin/prettierd",
    "start": "node bin/prettierd start",
    "prettier:check": "prettier --check .",
    "prettier:fix": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fsouza/prettierd.git"
  },
  "author": "Francisco Souza",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fsouza/prettierd/issues"
  },
  "homepage": "https://github.com/fsouza/prettierd",
  "devDependencies": {
    "@types/node": "^22.13.1",
    "@types/prettier": "^3.0.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "core_d": "^6.1.0",
    "prettier": "^3.5.0"
  },
  "files": [
    "bin",
    "dist",
    "LICENSE",
    "README.md"
  ],
  "optionalDependencies": {
    "@babel/parser": "^7.26.8",
    "@typescript-eslint/typescript-estree": "^8.24.0"
  }
}
